{
  "name": "sistema-gestao-escolar-frontend",
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:monitor": "node scripts/build-monitor.js build",
    "build:ci": "node scripts/build-monitor.js ci-report",
    "analyze": "npm run build:stats && npx webpack-bundle-analyzer dist/sistema-gestao-escolar-frontend/stats.json",
    "analyze:server": "npm run build:stats && npx webpack-bundle-analyzer dist/sistema-gestao-escolar-frontend/stats.json --mode server --port 8888",
    "bundle-report": "npm run build:stats && node scripts/bundle-report.js",
    "test:bundle": "npm run build:stats && node scripts/test-bundle-size.js",
    "test:bundle-only": "node scripts/test-bundle-size.js",
    "test:performance": "npm run build:stats && node scripts/test-performance-regression.js",
    "test:performance-only": "node scripts/test-performance-regression.js",
    "test:functional": "node scripts/run-functional-tests.js",
    "test:network": "node scripts/test-network-conditions.js",
    "test:all-validation": "npm run test:functional && npm run test:network",
    "validate:optimizations": "node scripts/validate-optimizations.js",
    "test:final-validation": "node scripts/final-validation-test.js",
    "dashboard": "node scripts/performance-dashboard.js",
    "dashboard:open": "npm run dashboard && start performance-dashboard.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:sistema-gestao-escolar-frontend": "node dist/sistema-gestao-escolar-frontend/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.4",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/entity": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "@primeng/themes": "^20.0.0",
    "chart.js": "^4.5.0",
    "express": "^4.18.2",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
